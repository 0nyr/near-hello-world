  "bugs": {
    "url": "https://github.com/0nyr/near-hello-world/issues"
  },
  "homepage": "https://github.com/0nyr/near-hello-world#readme",
  "dependencies": {
    "near-api-js": "^0.44.2",
    "regenerator-runtime": "^0.13.9"
  }
}
